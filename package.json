{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:uat": "vue-tsc --noEmit && vite build --mode uat",
    "docs": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx,.vue ",
    "lint:fix": "eslint --ext .ts,.tsx,vue src/** --no-error-on-unmatched-pattern --quiet --fix",
    "lint:js": "eslint --ext .js,.jsx.ts,.tsx,vue src/** --no-error-on-unmatched-pattern --quiet",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "precommit": "lint-staged",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "ant-design-vue": "^3.2.20",
    "axios": "^0.27.2",
    "chart.js": "^4.2.1",
    "dayjs": "^1.11.7",
    "nanoid": "^4.0.2",
    "pinia": "^2.0.13",
    "vite": "^3.0.7",
    "vue": "^3.2.33",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.7",
    "chalk": "4",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "4.3.7",
    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.24.1",
    "vite-plugin-eslint": "1.6.0",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.34.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/verifyCommit.js"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": "npm run lint:js",
    "**/*.{js,jsx,tsx,ts,vue,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
